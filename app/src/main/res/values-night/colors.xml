<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--应用程序的主品牌颜色。默认情况下，这是应用到操作栏背景-->
    <color name="colorPrimary">#186DB0</color>
    <!--状态栏颜色-->
    <!--主品牌颜色的深色变体。默认情况下，这是应用到的颜色状态栏(通过statusBarColor)和导航栏(通过navigationBarColor)。-->
    <color name="colorPrimaryDark">#0D2437</color>
    <!--控制各个控件被选中时的颜色,默认的光标等-->
    <!--明亮的色彩与品牌主色调相得益彰。默认情况下，这是应用的颜色到框架控件(通过colorControlActivated)。-->
    <color name="colorAccent">#DF972D</color>

    <color name="textColorPrimary">#B1B1B1</color>
    <color name="textColorSecondary">#7C7B7B</color>
    <color name="textColorThird">#5E5E5E</color>

    <color name="bgColorPrimary">#686464</color>
    <color name="bgColorSecondary">#292929</color>
    <color name="bgColorThird">#262626</color>

    <color name="colorRipple">#3F3F3F</color>
    <color name="colorBadge">#F44336</color>
</resources>
